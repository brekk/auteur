{
  "name": "auteur",
  "version": "0.0.1",
  "description": "auteur is the management interface for automating and generating raconteur instances.",
  "main": "index.js",
  "directories": {
    "lib": "./lib",
    "src": "./src"
  },
  "scripts": {
    "build": "./node_modules/.bin/coffee --compile --output lib/ src/",
    "build-test": "./node_modules/.bin/coffee --compile --output test/ src/test/",
    "test": "npm run build-test && mocha --reporter spec",
    "preinstall": "npm run build",
    "clean": "(rm src/*.js || true) && (rm src/test/*.js || true) && (rm -Rf lib || true)"
  },
  "bin": {
    "auteur": "./bin/auteur"
  },
  "keywords": [
    "auteur",
    "raconteur",
    "cli",
    "automator",
    "manager",
    "instance"
  ],
  "author": "Brekk Bockrath",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:brekk/auteur.git"
  },
  "bugs": {
    "url": "https://github.com/brekk/auteur/issues"
  },
  "homepage": "https://github.com/brekk/auteur",
  "dependencies": {
    "chalk": "^0.5.1",
    "fstream": "^1.0.2",
    "interpret": "^0.3.7",
    "liftoff": "^0.13.5",
    "lodash": "^2.4.1",
    "minimist": "^1.1.0",
    "parkplace": "^0.0.8",
    "pkginfo": "^0.3.0",
    "promised-io": "^0.3.4",
    "semver": "^4.1.0",
    "simple-glob": "^0.1.0",
    "tar": "^1.0.2"
  },
  "devDependencies": {
    "coffee-script": "^1.8.0",
    "should": "^4.1.0"
  }
}
